{
    "name": "mohanedghawar/countries-info",
    "description": "A comprehensive PHP library for accessing detailed country information, including regions, capitals, continents, and more.",
    "type": "library",
    "version": "1.0.1",
    "license": "MIT",
    "authors": [
        {
            "name": "MohaNed Ghawar",
            "email": "mohaned.ghawar@gmail.com"
        }
    ],
    "minimum-stability": "stable",
    "require": {
        "php": ">=7.4"
    },
    "autoload": {
        "psr-4": {
            "MohanedGhawar\\CountriesInfo\\": "src/"
        }
    },
    "require-dev": {
        "phpunit/phpunit": "^9.0"
    },
    "keywords": [
        "countries",
        "regions",
        "states",
        "provinces",
        "geography",
        "nations",
        "country-data",
        "world-data",
        "capitals",
        "continents",
        "population",
        "international",
        "global"
    ]
}
